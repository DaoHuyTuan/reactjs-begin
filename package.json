{
  "name": "reactjs-beginning",
  "version": "0.1.0",
  "author": "hunghk",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "eslint-config-react-app": "^1.0.5",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "method-override": "^2.3.9",
    "mongoose": "^4.11.5",
    "morgan": "^1.8.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cross-env": "^5.0.4",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "pre-commit": "^1.2.2",
    "promise": "7.1.1",
    "react": "^15.6.1",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-helmet": "^5.1.3",
    "react-router-dom": "^4.1.2",
    "sass-loader": "^6.0.6",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0"
  },
  "scripts": {
    "start": "cross-env BASE_URL=http://localhost:3001 node scripts/start.js",
    "eslint": "eslint --ignore-path .gitignore app/** server/**",
    "server": "cross-env NODE_ENV=development nodemon server",
    "build": "node scripts/build.js",
    "build:start": "cross-env NODE_ENV=production PORT=3003 nodemon server",
    "prebuild:start": "cross-env BASE_URL=http://localhost:3003 node scripts/build.js",
    "build:dev": "cross-env BASE_URL=http://localhost:3003 node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "prod": "cross-env NODE_ENV=production PORT=3003 nodemon server"
  },
  "pre-commit": "eslint",
  "keywords": [
    "express",
    "webpack",
    "reactjs",
    "react-begin",
    "nodejs",
    "react-node"
  ],
  "proxy": "http://localhost:3000",
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.js?(x)",
      "<rootDir>/app/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
